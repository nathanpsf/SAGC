
$(document).ready(function(){
 

	//ao iniciar a pagina esconder tudo menos a front inicial
	$("#blocoOcorrencias").hide();
	$("#blocoFichas").hide();
	
	
	$("#landingPageBotaoComecar").click(function () {
		$("#blocoOcorrencias").fadeIn(400);
        $("#landingPage").fadeOut(100);

		//TO COLOCANDO AQUI PRA TB DAR SHOW NA GRADE DE RESPONDER OCORRENCIAS NESSE BOTAO MAS O CERTO É SÓ DAR .SHOW() SE FOR UM USUARIO LOGADO
		$("#blocoFichas").fadeIn(400);
    });

	$( "td" ).click(function() {
	alert( "Handler for .click() called." );
	});
});
